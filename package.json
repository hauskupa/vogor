{
  "name": "vogor-webflow-build",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "dev": "npx esbuild src/main.js --bundle --format=iife --target=es2019 --sourcemap --outfile=dist/main.dev.js --watch",
    "release": "node release.js patch",
    "purge": "node purge.js",
    "deploy": "npm run build && git add -A && git diff --cached --quiet || git commit -m \"Auto deploy\" && git push origin main && npm run purge && npm run release",
    "build:purge": "npm run build && npm run purge",
    "build:icons": "svg-sprite-generate -d ./icons -o ./dist/sprite.svg"
  },
  "devDependencies": {
    "esbuild": "^0.25.9"
  },
  "dependencies": {
    "howler": "^2.2.4",
    "lottie-web": "^5.13.0"
  }
}
